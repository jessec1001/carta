    "three": "^0.133.1"
  },
  "devDependencies": {
    "@types/d3": "^7.0.0",
    "@types/three": "^0.133.0",
    "npm-watch": "^0.11.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}